g_pkg_flg = -bs-package-name your-project-name -bs-ns YourProjectName
src_root_dir = /Users/woonki/Documents/projects/gatsby-reason-starter
bsc = /Users/woonki/Documents/projects/gatsby-reason-starter/node_modules/bs-platform/darwin/bsc.exe
bsdep = /Users/woonki/Documents/projects/gatsby-reason-starter/node_modules/bs-platform/darwin/bsb_helper.exe
warnings = 
bsc_flags = -bs-super-errors
ppx_flags = 
g_dpkg_incls = 
g_ns = -bs-ns YourProjectName
g_lib_incls = -I . -I src/components/Greeting -I src/components -I src/pages -I src -I /Users/woonki/Documents/projects/gatsby-reason-starter/node_modules/reason-react/lib/ocaml
rule build_ast_from_re
  command = $bsc  $warnings -bs-jsx 3 $bsc_flags -o $out -bs-syntax-only -bs-binary-ast $in
  description = [34mBuilding[39m [2m${out}[22m
build  src/components/Greeting/Greeting.reast : build_ast_from_re $src_root_dir/src/components/Greeting/Greeting.re
rule mk_deps
  command = $bsdep -hash 814dd84b71ef537acc93b55bd7899543 $g_ns $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/components/Greeting/Greeting.d : mk_deps src/components/Greeting/Greeting.reast
rule ml_cmj_cmi
  command = $bsc $g_pkg_flg -bs-suffix $g_lib_incls $warnings $bsc_flags -o $out $in
  dyndep = $in_e.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/components/Greeting/Greeting-YourProjectName.cmj |  src/components/Greeting/Greeting-YourProjectName.cmi $src_root_dir/src/components/Greeting/Greeting.bs.js : ml_cmj_cmi src/components/Greeting/Greeting.reast ||  src/components/Greeting/Greeting.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output es6:src/components/Greeting
rule build_package
  command = $bsc -w -49 -color always -no-alias-deps  $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  YourProjectName.cmi : build_package YourProjectName.mlmap
