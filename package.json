{
  "name": "gatsby-reason-starter",
  "private": true,
  "description": "A Gatsby starter with reason & apollo-client",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "re:build": "bsb -make-world -clean-world",
    "re:watch": "bsb -make-world -clean-world -w"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "es6-promise": "^4.2.8",
    "gatsby": "^2.24.47",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reason-apollo-client": "0.0.1-beta.7",
    "reason-react": "^0.9.1"
  },
  "devDependencies": {
    "@reasonml-community/graphql-ppx": "^1.0.0-beta.22",
    "bs-platform": "^8.2.0",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattdamon108/gatsby-reason-starter"
  },
  "bugs": {
    "url": "https://github.com/mattdamon108/gatsby-reason-starter/issues"
  }
}
